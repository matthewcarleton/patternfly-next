{{#if page-template-header-brand--toggle}}
  {{#> page-header-brand-toggle}}
    {{#> button button--modifier="pf-m-plain"
      button--attribute='id="nav-toggle"
                      aria-label="Toggle primary navigation"' 
      button--HasBarsIcon="true"}}
    {{/button}}
  {{/page-header-brand-toggle}}
{{/if}}
{{#> page-header-brand-link page-header-brand-link--href="#"}}
  {{#> brand }}
    {{#> brand-source brand-source--attribute='media="(min-width: 765px)" srcset="/assets/images/l_pf-reverse-2.svg" alt="Patternfly Logo"'}}{{/brand-source}}
    {{#> brand-default-image brand-default-image--attribute='src="/assets/images/pf_mini_logo_white.svg" alt="Patternfly Logo"'}}{{/brand-default-image}}
  {{/brand}}
{{/page-header-brand-link}}
