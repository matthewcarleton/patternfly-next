{{#> chip-group chip-group--modifier="pf-m-toolbar" chip-group--id="toolbar-collapsed"}}
  {{#> chip-group-list-item }}
    {{#> chip-group-label}}
      Category One
    {{/chip-group-label}}
    {{#> chip-group newcontext}}
      {{#> chip}}
        {{#> chip-text chip-text--attribute=(concat 'id="' chip-group--id 'chip_one"')}}
          Chip One
        {{/chip-text}}
        {{#> button button--modifier="pf-m-plain" button--attribute=(concat 'aria-labelledby="' chip-group--id 'remove_chip_one ' chip-group--id 'chip_one" aria-label="Remove" id="' chip-group--id 'remove_chip_one"')}}
          {{> button-icon button-icon--type="times-circle"}}
        {{/button}}
      {{/chip}}
      {{#> chip}}
        {{#> chip-text chip-text--attribute=(concat 'id="' chip-group--id 'chip_two"')}}
          Chip Two
        {{/chip-text}}
        {{#> button button--modifier="pf-m-plain" button--attribute=(concat 'aria-labelledby="' chip-group--id 'remove_chip_two ' chip-group--id 'chip_two" aria-label="Remove" id="' chip-group--id 'remove_chip_two"')}}
          {{> button-icon button-icon--type="times-circle"}}
        {{/button}}
      {{/chip}}
      {{#> chip}}
        {{#> chip-text chip-text--attribute=(concat 'id="' chip-group--id 'chip_three"')}}
          Chip Three
        {{/chip-text}}
        {{#> button button--modifier="pf-m-plain" button--attribute=(concat 'aria-labelledby="' chip-group--id 'remove_chip_three ' chip-group--id 'chip_three" aria-label="Remove" id="' chip-group--id 'remove_chip_three"')}}
          {{> button-icon button-icon--type="times-circle"}}
        {{/button}}
      {{/chip}}
      {{#> chip}}
        {{#> chip-text chip-text--attribute=(concat 'id="' chip-group--id 'chip_four"')}}
          Chip Four
        {{/chip-text}}
        {{#> button button--modifier="pf-m-plain" button--attribute=(concat 'aria-labelledby="' chip-group--id 'remove_chip_four ' chip-group--id 'chip_four" aria-label="Remove" id="' chip-group--id 'remove_chip_four"')}}
          {{> button-icon button-icon--type="times-circle"}}
        {{/button}}
      {{/chip}}
    {{/chip-group}}
  {{/chip-group-list-item}}
{{/chip-group}}
{{#> chip chip--type="div" chip--modifier="pf-m-overflow"}}
  {{#> button button--modifier="pf-m-plain"}}
    {{#> chip-text}}
      3 more
    {{/chip-text}}
  {{/button}}
{{/chip}}

